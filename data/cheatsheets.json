        {"cmd": "wget", "desc": "Download file", "example": "wget https://example.com/file.zip"},
        {"cmd": "curl", "desc": "Transfer data", "example": "curl https://api.example.com"},
        {"cmd": "ssh", "desc": "Remote login", "example": "ssh user@host"},
        {"cmd": "scp", "desc": "Secure copy", "example": "scp file.txt user@host:/path/"},
        {"cmd": "netstat", "desc": "Network stats", "example": "netstat -tuln"},
        {"cmd": "ss", "desc": "Socket stats", "example": "ss -tuln"}
      ]
    },
