        {"cmd": "kill", "desc": "Kill process", "example": "kill -9 1234"},
        {"cmd": "killall", "desc": "Kill by name", "example": "killall firefox"},
        {"cmd": "jobs", "desc": "Background jobs", "example": "jobs"},
        {"cmd": "fg", "desc": "Foreground job", "example": "fg %1"},
        {"cmd": "bg", "desc": "Background job", "example": "bg %1"}
