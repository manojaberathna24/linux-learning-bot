        {"cmd": "sudo", "desc": "Run as superuser", "example": "sudo apt update"},
        {"cmd": "whoami", "desc": "Current username", "example": "whoami"},
        {"cmd": "id", "desc": "User ID info", "example": "id"},
        {"cmd": "passwd", "desc": "Change password", "example": "passwd"}
      ]
    },
    {
